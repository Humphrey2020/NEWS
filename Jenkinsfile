pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building the first Applications'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing the application i build'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying the application that was build'
            }
        }
    }
}

